  <fieldset id="F-Partnership">
  <!-- Part Name -->
  <legend>F. Partnerships</legend>
  <input type="hidden" id="partnerSum"  name="partnerSum" value="4" />

  <div class="row"><p class="sep"></p>
    <div class="span8">
        <div class="control-group">
          <label class="control-label"><span class="badge pull-left">F.1</span> Which (if any) actor(s) were partnered with?</label>

          <div class="controls">
            <label class="radio inline">
              <input type="radio" name="hasPartner" value="true">
              Yes
            </label>
            <label class="radio inline">
              <input type="radio" name="hasPartner" value="false">
              No
            </label>
          </div>
        </div>
        {% assign quantity = 12 %}
        {% for i in (2..quantity) %}
          {% assign fIndex = i %}
          {% include "submit-sections/F-pane" %}
        {% endfor %}
    </div>
    <div class="span4">
        <p class="help-block">Answers may include multiple responses</p>
    </div>
  </div>
  <!-- Text input-->
  <div id="newPartner" class="row" style="display: none;">
    <div class="span8"><p class="sep"></p>
          <div class="control-group">
            <div class="controls">
              <div class="btn-group pull-left">
                <a href="#" id="Partner-add" class="btn btn-mini">Add Partner</a>
                <a href="#" id="Partner-rem" class="btn btn-mini disabled">Remove Partner</a>
              </div>
            </div>
          </div>
     </div>
    <div class="span4">
          <p class="help-block"></p>
    </div>
  </div>

  </fieldset>